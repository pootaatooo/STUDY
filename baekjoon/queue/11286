#include <iostream>
#include <queue>
#include <cstdlib>
#include <algorithm>

using namespace std;

int main() {
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    int n, x;
    cin >> n;
    priority_queue<int> q;
    
    for(int i=0;i<n;i++) {
        cin >> x;
        int a = abs(x);
        
        if(a != 0) {
            int mini = min(x,a);
            q.push(mini);
        }
        else if(x == 0) {
            if(!q.empty()) {
                cout << q.top() << '\n';
                q.pop();
            }
            else
                cout << "0\n";
        }

    }

}

좀만 더 천천히 생각하면서 풀어라ㅓ
