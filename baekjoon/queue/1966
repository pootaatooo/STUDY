#include <iostream>
#include <queue>
#include <algorithm>

using namespace std;

int main() {
    int t;
    cin >> t;
    
    while(t--) {
        int n, m;
        cin >> n >> m;
        vector<pair<int,int>>v;
        priority_queue<int> pq;
        
        for(int i=0;i<n;i++) {
            int imp;
            cin >> imp;
            v.push_back({imp,i}); // 중요도와 그 순서를 넣어줌
            pq.push(imp);
        }
        
        
    }
    

}
