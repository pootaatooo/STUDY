#include <iostream>
#include <queue>
#include <algorithm>

using namespace std;

int main() {
    int t;
    cin >> t;
    
    while(t--) {
        int n, m;
        cin >> n >> m;
        
        queue<pair<int,int>>q;
        priority_queue<int> pq;
        
        for(int i=0;i<n;i++) {
            int imp;
            cin >> imp;
            q.push({imp,i});
            pq.push(imp);
        }
        
        for(int i=0;i<n;i++) {
            cout << q[i].front().first << ' ' << pq[i].front() << endl;
            q.pop(); pq.pop();
        }
        
        
        
    }


다 포기하고 우선순위 큐로 진행한다
    
    
    
    
    
    
    
    
    
    
}
