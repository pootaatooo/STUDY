#include <iostream>
#include <string>
#include <cmath>
#include <vector>

using namespace std;

int main() {
    string s;
    
    while(getline(cin, s)) {
        
        if(s == ".") continue;  // && s.length() == 1
        
        int l = s.length();
        int x = 0; // x는 소괄호
        int y = 0; // y는 대괄호
        int b[l+1] = {0,}; // 
        
        
        for(int i=0;i<l;i++) {
            if(s[i] == ')') {
                x--;
                b[i]--;
            }
            else if(s[i] == '(') {
                x++;
                b[i]++;
            }
            else if(s[i] == ']') {
                y--;
                b[i] -= 2;
            }
            else if(s[i] == '[') {
                y++;
                b[i] += 2;
            }
            
            if(x != 0 || y != 0) continue;
            
        }
        
        vector<int> v;
        int a[l+1] = {0,}; // 여기서 증감
        int k = 0; // 배열의 길이
        
        for(int i=0;i<l;i++) {
            if(b[i] == 0) continue;
            else {
                v.push_back(b[i]); // 벡터에 괄호듶의 값만 넣어줌
                a[k] = b[i];
                k++;
            }
        }
        
        // for(auto it = v.begin(); it != v.end(); it++) { // 벡터 반복문
        //     cout << *it << ' ';
        // }
        // cout << endl;
        
        a[-1] = 0;
        int re = 0;
        
        for(int i=0;i<k;i++) {
            if(a[i] < 0 && abs(a[i-1]) == a[i]) {
                a[i] = a[i-1] = 0;
            }
            else re = -1;
        }
        
        
        for(int i=0;i<k;i++) {
            re += a[i]; 
        }
        // if(re == 0) cout << "yes" << '\n';
        // else cout << "no" << '\n';

        
        if(x == 0 && y == 0) cout << "yes" << '\n';
        else cout << "no" << '\n';
        
    }
    
    
    
    
    
}
