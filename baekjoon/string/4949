#include <iostream>

using namespace std;

int main() {
    string s;
    
    while(getline(cin,s)) {
        if(s == ".") continue; // 점 하나만 나온 경우 출력 안해줌
        
        int l = s.length();
        int k = 0;
        int a[l+1] = {0,}; // a배열에 괄호에 해당하는 수를 넣어준다 ( = 1, ) = -1, [ = 2, ] = -2
        
        for(int i=0;i<l;i++) {
            if(s[i] == '(') {
                a[k] = 1;
                k++;
            }
            else if(s[i] == ')') {
                a[k] = -1;
                k++;
            }
            else if(s[i] == '[') {
                a[k] = 2;
                k++;
            }
            else if(s[i] == ']') {
                a[k] = -2;
                k++;
            }
 
        }
        
        bool re = true;
        int q = 1;
        int b[k+1] = {0,};
        b[0] = 0;
        
        for(int i=0;i<k;i++) {
            
            if(a[i] > 0) {
                b[q] = a[i];
                q++;
            }
            
            else if(a[i] == -1) {
                if(b[q-1] != 1) {
                    re = false;
                    break;
                }
                b[q-1] = 0;
                b[q--];
            }
            else if(a[i] == -2) {
                if(b[q-1] != 2) {
                    re = false;
                    break;
                }
                b[q-1] = 0;
                b[q--];
            }
        }
        int sum = 0;
        
        for(int i=1;i<k+1;i++) {
            sum += b[i];
            //cout << b[i] << ' ';
        }
        //cout << endl;
        
        if(re == false || sum != 0) cout << "no" << '\n';
        else cout << "yes" << '\n';
        
    }
    

    
}
