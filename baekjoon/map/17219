#include <iostream>
#include <unordered_map>

using namespace std;

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int n, m;

    cin >> n >> m;

    unordered_map<string, string> passwordMap;

    for (int i = 0; i < n; i++) {
        string loc, pass;
        cin >> loc >> pass;
        passwordMap[loc] = pass;
    }

    for (int i = 0; i < m; i++) {
        string res;
        cin >> res;

        auto it = passwordMap.find(res);
        if (it != passwordMap.end()) {
            cout << it->second << '\n';
        }
    }

    return 0;
}

gpt ver.

#include <iostream>

using namespace std;

int main() {
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    
    int n, m;
    
    cin >> n >> m;
    
    string loc[n];
    string pass[n];
    string res;
    
    for(int i=0;i<n;i++) {
        cin >> loc[i] >> pass[i];
    }
    
    for(int i=0;i<m;i++) {
        cin >> res;
        for(int j=0;j<n;j++) {
            if(res == loc[j]) {
                cout << pass[j] << '\n';
            }
        }
    }

}
