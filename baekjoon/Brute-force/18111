#include <iostream>

using namespace std;

int main() {
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0); // 시간제한 주의
    
    int n, m, b;
    cin >> n >> m >> b;
    
    int block[n][m] = {0,};
    
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            cin >> block[i][j];
        }
    }
    
     for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            cout << block[i][j] << ' ';
        }
        cout << endl;
    }

    // 땅파기
    
    
    // 땅쌓기
    
    
}
