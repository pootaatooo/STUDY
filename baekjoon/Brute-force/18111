#include <iostream>

using namespace std;

int main() {
    ios:sync_with_stdio(0); cin.tie(0); cout.tie(0);
    
    int n, m, b;
    cin >> n >> m >> b;
    int map[501][501];
    
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            cin >> map[i][j];
        }
    }
    
    
    
}
