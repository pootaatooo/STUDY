#include <iostream>
#include <queue>

using namespace std;

int n, m;
int dx[4] = {0,-1,0,1};
int dy[4] = {-1,0,1,0};
int map[1001][1001];
int cnt[1001][1001]; // 여기 거리를 저장해줌
bool visited[1001][1001];

typedef pair<int,int> pii;

void bfs(int x, int y) { // x,y는 목표지점의 좌표
    queue<pii> q;
    
    // for(int i=0;i<n;i++) { // cnt 초기화부분
    //     for(int j=0;j<m;j++) {
    //         cnt[i][j] = 0;
    //     }
    // }
    
    q.push({x,y});
    visited[x][y] = true;
    
    while(!q.empty()) {
        pii cur = q.front();
        q.pop();
        
        for(int i=0;i<4;i++) {
            int nx = cur.first + dx[i];
            int ny = cur.second + dy[i];
            
            if(nx>=0 && ny>=0 && nx<n && ny<m && !visited[nx][ny] && map[nx][ny] == 1) {
                visited[nx][ny] = true;
                q.push({nx,ny});
                cnt[nx][ny]++;
            }
        }
    }
    
    // for(int i=0;i<n;i++) { // cnt 초기화부분
    //     for(int j=0;j<m;j++) {
    //         cnt[i][j] = 0;
    //     }
    // }
    
}

int main() {
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    
    cin >> n >> m;
    
    int u = 0, v = 0;
    
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            cin >> map[i][j];
        }
    }
    
    // for(int i=0;i<n;i++) {
    //     for(int j=0;j<m;j++) {
    //         cout << map[i][j] << ' ';
    //     }
    //     cout << endl;
    // }
    
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            if(map[i][j] == 2) {
                u = i;
                v = j;
            }
        }
    }
    
    bfs(u, v); // 목표지점을 인수로 보내줌
    
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            cout << cnt[i][j] << ' ';
        }
        cout << '\n';
    }
    
    return 0;
}
