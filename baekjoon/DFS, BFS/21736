#include <iostream>

using namespace std;

int main() {
    int n, m;
    cin >> n >> m;
    char x[n][m];
    
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            cin >> x[i][j]; 
        }
    }
    
    // for(int i=0;i<n;i++) {
    //     for(int j=0;j<m;j++) {
    //         cout << x[i][j]; 
    //     }
    //     cout << '\n';
    // }
    int start_n = 0, start_m = 0;
    
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            if(x[i][j] == 'I') {
                start_n = i;
                start_m = j;
            } 
        }
    }
    
    cout << start_n << ' ' << start_m;
    
}
