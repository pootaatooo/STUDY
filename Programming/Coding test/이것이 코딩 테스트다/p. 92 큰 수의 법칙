

#include <iostream>
#include <algorithm>

using namespace std;

int main() {
    int n, m, k;
    cin >> n >> m >> k;
    int a[n];
    
    for(int i=0;i<n;i++) {
        cin >> a[i];
    }
    
    sort(a, a + n);
    int res = 0;
    
    for(int i=0;i<m;i += k) {
        
        for(int j=0;j<k;j++) {
            res += a[n-1];
        }   
        cout << res << endl;
        res += a[n-2];
    }
    
    cout << res;
    
}
